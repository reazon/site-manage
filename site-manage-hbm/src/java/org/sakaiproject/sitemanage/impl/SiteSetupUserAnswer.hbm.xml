<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping package="org.sakaiproject.sitemanage.impl">
	<class name="SiteSetupUserAnswerImpl"
		table="SSQ_USER_ANSWER">
		
		<id name="id" length="99" column="ID">
			<generator class="org.sakaiproject.id.hbm.IdGenerator"/>
		</id>
		
		<property name="siteId" type="string" not-null="true">
            <column name="SITE_ID"/>
        </property>
        
        <property name="userId" type="string" not-null="true">
            <column name="USER_ID"/>
        </property>
        
        <property name="answerString" type="string" not-null="false">
            <column name="ANSWER_STRING"/>
        </property>
        
        <many-to-one name="questionId" column="QUESTION_ID"
			class="SiteSetupQuestionImpl" index="SSQ_USER_ANSWER_QUESTION_I" lazy="false" />
			
		<many-to-one name="answerId" column="ANSWER_ID"
			class="SiteSetupQuestionImpl" index="SSQ_USER_ANSWER_ANSWER_I" lazy="false" />
    </class>
</hibernate-mapping>